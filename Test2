pipeline {
    agent any

    stages {
        stage('A') {
            steps {
                echo 'Hello World'
            }
        }
        stage('B') {
            steps {
                echo 'Building'
            }
        }   
        stage('C') {
            steps {
                echo 'Deploying'
            }
        }   
        stage('D') {
            steps {
                echo 'Testing'
            }
        }
        stage('E') {
            steps {
                echo 'Releasing'
            }
        }
    }
}
